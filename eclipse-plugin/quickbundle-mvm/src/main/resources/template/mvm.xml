<?xml version="1.0" encoding="UTF-8"?>
<rules rulesVersion="4.0.0">
	<codegen>
		<!--定义要生成的文件列表，分java、jsp、配置文件3种-->
		<files filesType="java">
			<file xsltPath="JavaEE-standard/java/web/TableNameAction.xsl" targetPath="web/TableNameAction.java" bundleCode="main"/>
			<file xsltPath="JavaEE-standard/java/web/TableNameAjaxAction.xsl" targetPath="web/TableNameAjaxAction.java" bundleCode="ajax"/>
			<file xsltPath="JavaEE-standard/java/web/TableNameConditionAction.xsl" targetPath="web/TableNameConditionAction.java" bundleCode="condition"/>
			<file xsltPath="JavaEE-standard/java/web/TableNameReadOnlyAction.xsl" targetPath="web/TableNameReadOnlyAction.java" bundleCode="readonly"/>
			<file xsltPath="JavaEE-standard/java/service/impl/TableNameService.xsl" targetPath="service/impl/TableNameService.java"/>
			<file xsltPath="JavaEE-standard/java/service/ITableNameService.xsl" targetPath="service/ITableNameService.java"/>
			<file xsltPath="JavaEE-standard/java/dao/impl/TableNameDao.xsl" targetPath="dao/impl/TableNameDao.java"/>
			<file xsltPath="JavaEE-standard/java/dao/ITableNameDao.xsl" targetPath="dao/ITableNameDao.java"/>
			<file xsltPath="JavaEE-standard/java/util/exception/TableNameException.xsl" targetPath="util/exception/TableNameException.java"/>
			<file xsltPath="JavaEE-standard/java/util/ITableNameConstants.xsl" targetPath="util/ITableNameConstants.java"/>
			<file xsltPath="JavaEE-standard/java/vo/TableNameVo.xsl" targetPath="vo/TableNameVo.java"/>
		</files>
		<files filesType="jsp">
			<file xsltPath="JavaEE-standard/jsp/ajax/listTableName.xsl" targetPath="ajax/listTableName.jsp" bundleCode="ajax"/>
			<file xsltPath="JavaEE-standard/jsp/condition/detailTableName.xsl" targetPath="condition/detailTableName.jsp" bundleCode="condition"/>
			<file xsltPath="JavaEE-standard/jsp/condition/insertTableName.xsl" targetPath="condition/insertTableName.jsp" bundleCode="condition"/>
			<file xsltPath="JavaEE-standard/jsp/condition/listTableName.xsl" targetPath="condition/listTableName.jsp" bundleCode="condition"/>
			<file xsltPath="JavaEE-standard/jsp/util/referenceTableName.xsl" targetPath="util/referenceTableName.jsp" bundleCode="reference"/>
			<file xsltPath="JavaEE-standard/jsp/util/statisticTableName_chart.xsl" targetPath="util/statisticTableName_chart.jsp" bundleCode="statistic"/>
			<file xsltPath="JavaEE-standard/jsp/util/statisticTableName_rowColumn.xsl" targetPath="util/statisticTableName_rowColumn.jsp" bundleCode="statistic"/>
			<file xsltPath="JavaEE-standard/jsp/util/statisticTableName_ofc.xsl" targetPath="util/statisticTableName_ofc.jsp" bundleCode="statistic"/>
			<file xsltPath="JavaEE-standard/jsp/util/statisticTableName_ofcData.xsl" targetPath="util/statisticTableName_ofcData.jsp" bundleCode="statistic"/>
			<file xsltPath="JavaEE-standard/jsp/detailTableName.xsl" targetPath="detailTableName.jsp" bundleCode="main"/>
			<file xsltPath="JavaEE-standard/jsp/exportTableName_custom.xsl" targetPath="exportTableName_custom.jsp" bundleCode="importExport"/>
			<file xsltPath="JavaEE-standard/jsp/exportTableName_excel.xsl" targetPath="exportTableName_excel.jsp" bundleCode="importExport"/>
			<file xsltPath="JavaEE-standard/jsp/importTableName.xsl" targetPath="importTableName.jsp" bundleCode="importExport"/>
			<file xsltPath="JavaEE-standard/jsp/insertTableName.xsl" targetPath="insertTableName.jsp" bundleCode="main"/>
			<file xsltPath="JavaEE-standard/jsp/listTableName.xsl" targetPath="listTableName.jsp" bundleCode="main"/>
			<file xsltPath="JavaEE-standard/jsp/middle/listMiddleTableName.xsl" targetPath="middle/" result-document="true" bundleCode="main"/>
		</files>
		<files filesType="config" appendPath="WEB-INF/config">
			<file xsltPath="JavaEE-standard/config/applicationContext.xsl" targetPath="spring/rm.applicationContext.xml" afterKeyWord="&lt;!--startProjectBeans-->"/>
			<file xsltPath="JavaEE-standard/config/struts-config.xsl" targetPath="struts/rm.struts-config.xml" afterKeyWord="&lt;action-mappings>"/>
			<file xsltPath="JavaEE-standard/config/jdbc.xsl" targetPath="jdbc/jdbc.properties" afterKeyWord="NOT_EXIST_POSITION"/>
			<file xsltPath="JavaEE-standard/config/id.xsl" targetPath="jdbc/id.xml" afterKeyWord="&lt;!--generateFromHere-->"/>
			<file xsltPath="JavaEE-standard/config/initCodeTypeData.xsl" targetPath="rm/initCodeTypeData.xml" afterKeyWord="&lt;codes>" rowIsUnique="true"/>
		</files>
		<files filesType="config" appendPath="jsp">
			<file xsltPath="JavaEE-standard/config/funcTree_default.xsl" targetPath="funcTree_default.jsp" afterKeyWord="language=&quot;java&quot; %>" bundleCode="main"/>
			<file xsltPath="JavaEE-standard/config/tree_default.xsl" targetPath="ajax/tree_default.jsp" afterKeyWord="language=&quot;java&quot; %>" bundleCode="ajax"/>
		</files>
		<files filesType="other">
			<file xsltPath="JavaEE-standard/other/IRmCodeConstants.xsl" targetPath="org/quickbundle/modules/code/IRmCodeConstants.java" afterKeyWord="//start dictionary define" rowIsUnique="true"/>
		</files>
	</codegen>
</rules>